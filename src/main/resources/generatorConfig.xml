		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.ahnstudio.management.dao"  targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

