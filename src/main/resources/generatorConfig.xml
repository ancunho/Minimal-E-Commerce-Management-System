		<javaTypeResolver >
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.ahnstudio.management.pojo" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
